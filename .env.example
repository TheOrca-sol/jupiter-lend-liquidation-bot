# Solana RPC endpoint (use a reliable RPC provider like Helius, QuickNode, or Triton)
RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Your wallet's private key (keep this secure!)
WALLET_PRIVATE_KEY=your_private_key_here

# Minimum profit threshold in USD (optional, defaults to 1)
MIN_PROFIT_USD=1

# Poll interval in milliseconds (defaults to 5000 - 5 seconds)
POLL_INTERVAL_MS=5000

# Enable verbose logging (true/false)
VERBOSE=true

# Telegram Bot Configuration (optional)
# Get bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Your Telegram chat ID (send /start to your bot and check the chat ID)
# Or use @userinfobot to get your chat ID
TELEGRAM_CHAT_ID=your_chat_id_here
